import pandas as pd
import matplotlib.pyplot as plt
import plotly.express as px

# Load data
data = pd.read_csv("../data/raw/population.csv")

# Matplotlib Chart
plt.plot(data["Year"], data["Population"])
plt.title("Kenya Population Growth")
plt.xlabel("Year")
plt.ylabel("Population")
plt.show()

# Interactive Plotly Chart
fig = px.line(data, x="Year", y="Population",
              title="Kenya Population Growth Interactive")

fig.show()
